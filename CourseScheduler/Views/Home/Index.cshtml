@{
    ViewBag.Title = "Home Page";
}

<main>
    <h1>Login</h1>
    <form id="login-form" action="/Home/Login" method="post">
        <label>
            <input v-model="user.username" placeholder="Username" name="username" required />
        </label>
        <label>
            <input v-model="user.password" placeholder="Password" name="password" required />
        </label>
        <button type="submit">Submit</button>
    </form>
</main>